  
pipeline {
    agent any
    stages {
        stage('Deploying to server') {
             steps {
                 sshPublisher(publishers: [sshPublisherDesc(configName: 'test-jenkins', transfers: [sshTransfer(cleanRemote: false, excludes: '', execCommand: 'ls -ltr', execTimeout: 120000, flatten: false, makeEmptyDirs: false, noDefaultExcludes: false, patternSeparator: '[, ]+', remoteDirectory: '/home/jenkinsuser', remoteDirectorySDF: false, removePrefix: '/var/lib/jenkins/workspace/ssh-test', sourceFiles: '/var/lib/jenkins/workspace/ssh-test/missingcode.tar.gz')], usePromotionTimestamp: false, useWorkspaceInPromotion: false, verbose: false)])
             }     
        }
    }
}

