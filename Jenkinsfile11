  
pipeline {
    agent any
    stages {
        stage('Deploying to server') {
             steps {
                 sshPublisher(publishers: [sshPublisherDesc(configName: 'test-jenkins', transfers: [sshTransfer(cleanRemote: false, excludes: '', execCommand: 'wc -l', execTimeout: 120000, flatten: false, makeEmptyDirs: false, noDefaultExcludes: false, patternSeparator: '[, ]+', remoteDirectory: '//home//ec2-user//rmr//target', remoteDirectorySDF: false, removePrefix: '', sourceFiles: 'missingcode.tar.gz')], usePromotionTimestamp: false, useWorkspaceInPromotion: false, verbose: false)])
             }     
        }
    }
}

